%body
  %header#top.navbar.navbar-static-top.subpage{:role => "banner"}
    .container
      .navbar-header
        %button.navbar-toggle{"data-target" => ".bs-navbar-collapse", "data-toggle" => "collapse", :type => "button"}
          %span.sr-only Toggle navigation
          %span.icon-bar
          %span.icon-bar
          %span.icon-bar
        = link_to @propertyinformation.name, "/", class: "navbar-brand"
      %nav.collapse.navbar-collapse.bs-navbar-collapse{:role => "navigation"}
        %ul.nav.navbar-nav.navbar-right
          - @pages.each do |page|
            %li= link_to page.title, pages_id_path(page)
          %li= link_to "Leasing", leasing_path
          = admin_link
