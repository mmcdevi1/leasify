.navbar.navbar-fixed-top{:role => "navigation"}
  .container
    .navbar-header
      %button.navbar-toggle{"data-target" => ".navbar-collapse", "data-toggle" => "collapse", :type => "button"}
        %span.sr-only Toggle navigation
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
      = link_to "Leasify", root_path,  class: "navbar-brand corner-store"
    .collapse.navbar-collapse.uppercase
      %ul.nav.navbar-nav.navbar-right.brandon-bold
        %li= link_to "Templates", templates_path
        - if user_signed_in?
          %li= link_to "Your Dashboard", dashboard_path
        - else
          %li= link_to "Sign Up", register_path
          %li= link_to "Login", new_user_session_path
    / /.nav-collapse
  / /.container
/ end nav bar
